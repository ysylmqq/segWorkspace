		                	<input type='text' id="remark" name='remark' class="form-control" />
		                </span>
		                <span style="width:60px;">APP登录</span>
		                <input type='radio' id="app" name="app" style='vertical-align:middle;' onclick="checkapp()"/>
		                <a href="javascript:void(0);" style="margin:0px;padding:0px;padding-left:10px;padding-top:5px;"><img alt="增加电话号码" src="../../images/form_add.png" title="增加电话号码" style="vertical-align:middle"></a>
		            </div>
		        </div>
            </div>
        </fieldset>

        <fieldset style="width:100%;border-top:0px;">	
	        <div class='submit'>
	        	<button id="nw_fast_submit" type="submit" class="btn btn-primary btn-xs" onclick="saveAndreset();">保存并新增</button>
	            <button id="nw_fast_submit" type="submit" class="btn btn-primary btn-xs" onclick="fast_save();">保存</button> 
	            <button id="nw_fast_cancel" type="button" class="btn btn-primary btn-xs" onclick="fast_reset();">重置</button>
	        </div>
        </fieldset>

	</form>
